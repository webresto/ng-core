{
  "name": "ng-core",
  "version": "0.0.10",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:core": "ng build @webresto/ng-core -c production",
    "build:cart": "ng build @webresto/ng-cart -c production",
    "build:user": "ng build @webresto/ng-user -c production",
    "build": "npm run build:core && npm run build:cart && npm run build:user",
    "pub:core": "cd dist/webresto/ng-core && npm publish --access=public",
    "pub:cart": "cd dist/webresto/ng-cart && npm publish --access=public",
    "pub:user": "cd dist/webresto/ng-user && npm publish --access=public",
    "pub": "npm run pub:core && npm run pub:cart && npm run pub:user",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^12.0.0",
    "@angular/compiler": "^12.0.0",
    "@angular/core": "^12.0.0",
    "@angular/forms": "^12.0.0",
    "@angular/platform-browser": "^12.0.0",
    "@angular/platform-browser-dynamic": "^12.0.0",
    "@angular/router": "^12.0.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.0",
    "@angular/cli": "^12.0.0",
    "@angular/compiler-cli": "^12.0.0",
    "@types/jasmine": "^3.7.2",
    "@types/node": "^15.0.3",
    "codelyzer": "^6.0.2",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "ng-packagr": "^12.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "^4.2.4"
  }
}
